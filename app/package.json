  "scripts": {
    "pre-commit": "lint-staged",
    "post-install": "patch-package",
    "build:android": "CONFIGURATION_SOURCE=Provided VITE_DEBUG=FALSE VITE_MOBILE=TRUE VITE_TARGET_PLATFORM=android vite build",
    "build:ios": "CONFIGURATION_SOURCE=Provided VITE_DEBUG=FALSE VITE_MOBILE=TRUE VITE_TARGET_PLATFORM=ios vite build",
    "test": "CONFIGURATION_SOURCE=Provided VITE_MOBILE=FALSE vitest --config vitest.config.ts",
    "state_test_coverage": "CONFIGURATION_SOURCE=Provided vitest run --coverage --coverage.include=state/**/*.ts --config vitest.config.ts",
    "state_test_coverage_gh": "CONFIGURATION_SOURCE=Provided vitest run --coverage --coverage.include=state/**/*.ts  --coverage.reporter=json-summary --coverage.reporter=json --config vitest.config.ts",
    "check-types": "tsc --noEmit --skipLibCheck",
    "lint:fix": "npm run lint -- --fix",
    "start": "CONFIGURATION_SOURCE=Provided ENABLE_WDYR=TRUE  VITE_DEBUG=TRUE  VITE_MOBILE=FALSE VITE_TARGET_PLATFORM=web vite serve --port 3000 --strictPort",
    "docker": "CONFIGURATION_SOURCE=Provided ENABLE_WDYR=TRUE  VITE_DEBUG=TRUE  VITE_MOBILE=FALSE VITE_TARGET_PLATFORM=web vite serve --port 3000 --host=0.0.0.0 --strictPort",
    "start:mobileMode": "CONFIGURATION_SOURCE=Provided ENABLE_WDYR=TRUE VITE_DEBUG=TRUE VITE_MOBILE=TRUE VITE_TARGET_PLATFORM=web vite serve --port 3000 --strictPort",
    "build": "NODE_OPTIONS='--max-old-space-size=8000' ENABLE_STATS=TRUE CONFIGURATION_SOURCE=Provided VITE_DEBUG=FALSE VITE_MOBILE=FALSE VITE_TARGET_PLATFORM=web vite build",
    "build-prod": "NODE_OPTIONS='--max-old-space-size=8000' CONFIGURATION_SOURCE=Caddy VITE_DEBUG=FALSE VITE_MOBILE=FALSE VITE_TARGET_PLATFORM=web vite build -l error",
    "assets": "npx @capacitor/assets generate --iconBackgroundColor '#fefefe' --iconBackgroundColorDark '#333333' --splashBackgroundColor '#fefefe' --splashBackgroundColorDark '#333333' --ios"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.test.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "/Users/rob/Clones/invasivesbc/app/src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  }
}
